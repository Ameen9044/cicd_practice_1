{
    "family": "first-run-task-definition",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sample-app-service",
            "image": "latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
